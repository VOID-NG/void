{
  "name": "void-marketplace-backend",
  "version": "1.0.0",
  "description": "Backend for Void Marketplace",
  "main": "src/server.js",
  "scripts": {
      "start": "node src/server.js",
      "dev": "nodemon src/server.js",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:coverage": "jest --coverage",
      "test:integration": "jest --testPathPattern=integration",
      "test:unit": "jest --testPathPattern=unit",
      "test:performance": "node tests/performance/search/searchLoadTest.js",
      "db:generate": "prisma generate",
      "db:migrate": "prisma migrate dev",
      "db:migrate:deploy": "prisma migrate deploy",
      "db:migrate:reset": "prisma migrate reset",
      "db:studio": "prisma studio",
      "db:seed": "node prisma/seed.js",
      "db:test:setup": "NODE_ENV=test prisma migrate deploy",
      "lint": "eslint src/ tests/ --ext .js,.json",
      "lint:fix": "eslint src/ tests/ --ext .js,.json --fix",
      "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
      "build": "echo 'Build completed'",
      "precommit": "npm run lint && npm run test:unit",
      "health": "node scripts/health-check.js",
      "search:test": "node scripts/test-search.js",
      "search:benchmark": "node scripts/benchmark-search.js",
      "ai:test": "node scripts/test-ai-services.js",
      "debug": "node --inspect src/server.js",
      "clean": "rm -rf node_modules package-lock.json && npm install",
      "docker:build": "docker build -t void-marketplace-backend .",
      "docker:run": "docker run -p 5000:5000 void-marketplace-backend"
},
  "keywords": [
    "marketplace",
    "ecommerce",
    "nigeria",
    "api",
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "ai-search"
  ],
  "author": "Void Marketplace Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "cloudinary": "^1.41.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "socket.io": "^4.8.1",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "axios": "^1.6.2",
    "crypto": "^1.0.1",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "express-validator": "^7.0.1",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "redis": "^4.6.12",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "aws-sdk": "^2.1515.0",
    "googleapis": "^131.0.0",
    "@google/generative-ai": "^0.24.1",
    "twilio": "^4.19.3",
    "stripe": "^14.9.0",
    "paystack": "^2.0.1",
    "flutterwave-node-v3": "^1.1.10",
    "opay-node": "^1.0.5",
    "payant": "^1.0.3",
    "vogue-pay": "^1.0.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.44",
    "slugify": "^1.6.6",
    "validator": "^13.11.0",
    "lodash": "^4.17.21",
    "fuse.js": "^7.0.0",
    "natural": "^6.8.0",
    "openai": "^4.20.1",
    "@huggingface/inference": "^2.6.4",
    "tensorflow": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "canvas": "^2.11.2",
    "pdf-parse": "^1.1.1",
    "mammoth": "^1.6.0",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "qrcode": "^1.5.3",
    "speakeasy": "^2.0.0",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "express-slow-down": "^2.0.1",
    "@sentry/node": "^7.85.0",
    "newrelic": "^11.7.0",
    "express-status-monitor": "^1.3.4",
    "prom-client": "^15.1.0",
    "express-prometheus-middleware": "^1.2.0",
    "bull": "^4.12.0",
    "form-data": "^4.0.3",
    "socket.io-client": "^4.8.1",
    "pgvector": "^0.1.3",
    "pusher": "^5.0.0"
  },
  "devDependencies": {
    "prisma": "^5.7.1",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.1.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "eslint": "^9.30.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "nyc": "^15.1.0",
    "faker": "^6.6.6",
    "@faker-js/faker": "^8.3.1",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "prisma": {
  "seed": "node prisma/seed.js"
},
"eslintConfig": {
      "extends": ["standard"],
      "env": {
        "node": true,
        "jest": true,
        "es2022": true
      },
      "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
      },
      "rules": {
        "no-console": "warn",
        "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
        "prefer-const": "error",
        "no-var": "error"
      }
    },
    "prettier": {
      "singleQuote": true,
      "semi": true,
      "tabWidth": 2,
      "trailingComma": "es5",
      "printWidth": 100
    },
    "nodemonConfig": {
      "watch": ["src"],
      "ext": "js,json",
      "ignore": ["tests/", "node_modules/"],
      "env": {
        "NODE_ENV": "development"
      }
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/VOID-NG/void"
    },
    "bugs": {
      "url": "https://github.com/VOID-NG/void/issues"
    },
    "homepage": "https://github.com/VOID-NG/void#readme"
}